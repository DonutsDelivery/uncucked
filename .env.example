# Discord Application (Bot + OAuth2)
DISCORD_TOKEN=your_bot_token
DISCORD_CLIENT_ID=your_client_id
DISCORD_CLIENT_SECRET=your_client_secret

# Server
PORT=3001
JWT_SECRET=change_this_to_a_long_random_string
CLIENT_URL=http://localhost:5173
SERVER_URL=http://localhost:3001

# Multiple domains (comma-separated, optional — defaults to CLIENT_URL)
# ALLOWED_ORIGINS=https://example.com,https://alt.example.com

# Admin user ID (Discord user ID) — required for multi-bot management
# ADMIN_USER_ID=your_discord_user_id
